<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
  xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
  xmlns:cq="http://www.day.com/jcr/cq/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Middle Nav Links" sling:resourceType="cq/gui/components/authoring/dialog">
  <content granite:class="cmp-author_editor" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
    <items jcr:primaryType="nt:unstructured">
      <tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs" maximized="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
          <multifieldwithmap jcr:primaryType="nt:unstructured" jcr:title="Multifield For Header Links" sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
              <well jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well" fieldLabel="Create Your Own Links">
                <items jcr:primaryType="nt:unstructured">
                  <linkwithdetails jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldDescription="Create Links" fieldLabel="Create Links" required="{Boolean}false">
                    <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" emptyText="Link Details" name="./linkwithdetails">
                      <items jcr:primaryType="nt:unstructured">
                        <linklabel jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Link Label" name="./linklabel" required="{Boolean}false"/>
                        <icon jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Label Icon" name="./icon">
                          <items jcr:primaryType="nt:unstructured">
                            <primary jcr:primaryType="nt:unstructured" text="message icon" value="icon-contattaci2"/>
                            <secondary jcr:primaryType="nt:unstructured" text="calendar icon" value="icon-calendar"/>
                            <tertiary jcr:primaryType="nt:unstructured" text="search icon" value="icon-search"/>
                            <fourth jcr:primaryType="nt:unstructured" text="earth icon" value="icon-estero2"/>
                          </items>
                        </icon>
                        <target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Target" name="./target">
                          <items jcr:primaryType="nt:unstructured">
                            <primary jcr:primaryType="nt:unstructured" text="blank" value="_blank"/>
                            <secondary jcr:primaryType="nt:unstructured" text="self" value="_self"/>
                          </items>
                        </target>
                        <link jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser" rootPath="/content/" fieldLabel="Link" name="./link" required="{Boolean}true"/>
                      </items>
                    </field>
                  </linkwithdetails>
                </items>
              </well>
            </items>
          </multifieldwithmap>
        </items>
      </tabs>
    </items>
  </content>
</jcr:root>