<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
  xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
  xmlns:cq="http://www.day.com/jcr/cq/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Footer Social Links" sling:resourceType="cq/gui/components/authoring/dialog">
  <content granite:class="cmp-author_editor" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
    <items jcr:primaryType="nt:unstructured">
      <tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs" maximized="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
          <multifieldwithmap jcr:primaryType="nt:unstructured" jcr:title="Multifield For Socials" sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
              <well jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well" fieldLabel="Create Your Own Links">
                <items jcr:primaryType="nt:unstructured">
                  <linkwithdetails jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldDescription="Create Links" fieldLabel="Create Links" required="{Boolean}false">
                    <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" emptyText="Link Details" name="./linkwithdetails">
                      <items jcr:primaryType="nt:unstructured">
                        <icon jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Link Label" name="./icon" required="{Boolean}true">
                          <items jcr:primaryType="nt:unstructured">
                            <primary jcr:primaryType="nt:unstructured" text="youtube icon" value="icon-youtube"/>
                            <secondary jcr:primaryType="nt:unstructured" text="instagram icon" value="icon-instagram"/>
                            <tertiary jcr:primaryType="nt:unstructured" text="linkedin icon" value="icon-linkedin"/>
                            <fourth jcr:primaryType="nt:unstructured" text="twitter icon" value="icon-twitter"/>
                            <fifth jcr:primaryType="nt:unstructured" text="facebook icon" value="icon-facebook"/>
                          </items>
                        </icon>
                        <link jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Link" name="./link" required="{Boolean}true"/>
                      </items>
                    </field>
                  </linkwithdetails>
                </items>
              </well>
            </items>
          </multifieldwithmap>
        </items>
      </tabs>
    </items>
  </content>
</jcr:root>